# BWGA Intelligence AI - Configuration Template
# Copy this file to .env.local and fill in your values

# Google Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Feature Flags
REACT_APP_USE_REAL_AI=true
REACT_APP_USE_REAL_DATA=true
REACT_APP_USE_REAL_BACKEND=false
REACT_APP_SHOW_DEMO_INDICATORS=false
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_AUTH=false

# Development Settings
NODE_ENV=development
VITE_API_BASE_URL=http://localhost:3001/api

# Self-Learning Configuration
VITE_ENABLE_SELF_LEARNING=true
VITE_LEARNING_DATA_PATH=./learning-data
VITE_AUTO_IMPROVE=true
